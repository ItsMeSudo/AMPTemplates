        "DisplayName": "Game Mode",
        "Category": "Call of Duty Black Ops II:stadia_controller",
        "Subcategory": "Server:dns:1",
        "Description": "Sets the game mode. Configure the relevant config file accordingly - 'Plutonium/storage/t6/dedicated.cfg' for Multiplayer, 'Plutonium/storage/t6/dedicated_zm.cfg' for Zombies",
        "Keywords": "game,mode,gamemode",
        "FieldName": "GameMode",
        "InputType": "enum",
        "ParamFieldName": "GameMode",
        "DefaultValue": "t6mp \"{{$FullBaseDir}}T6\" -dedicated +set key {{ServerKey}} +set fs_game {{FS_GAME}} +exec dedicated.cfg",
        "EnumValues": {
            "t6mp \"{{$FullBaseDir}}T6\" -dedicated +set key {{ServerKey}} +set fs_game {{FS_GAME}} +exec dedicated.cfg": "Multiplayer (default)",
            "t6zm \"{{$FullBaseDir}}T6\" -dedicated +set key {{ServerKey}} +set fs_game {{FS_GAME}} +exec dedicated_zm.cfg": "Zombies"
        }
    },
